.wux-cell-group__hd {
  font-size: 28rpx;
  color: #fff;
  width: 100%;
  box-sizing: border-box
}
.wux-cell-group__bd {
  position: relative;
  background-color: rgba(0, 0, 0, 0);
}
.wux-cell-group__bd::after {
  content: " ";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  transform: scale(.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
}
.wux-cell-group__ft {
  /* padding: 18rpx 30rpx 30rpx; */
  padding: 0rpx;
  font-size: 28rpx;
  color: #fff
}