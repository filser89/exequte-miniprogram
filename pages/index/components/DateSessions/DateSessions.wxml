<view>
  <view wx:if="{{sessions.length}}" >
    <view wx:for="{{sessions}}" wx:for-item="session" wx:for-index="i" wx:key="unique"  >
      <SessionItem session="{{session}}" />
    </view>
  </view>
  <view wx:else > 
    <view class=" tc db" style="height: 200px!important; background-color: black;  margin-top: 50px;  ">No sessions today </view>
  </view>
  
</view>