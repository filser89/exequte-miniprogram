/**app.wxss**/
@import 'stylingsheets/components/components';
@import 'stylingsheets/layout/layout';
@import 'font.wxss';
@import 'fa.wxss';

page {
  font-family: Nunito, Arial, Helvetica, sans-serif;
  box-sizing: border-box;
  height: 100%;
  background-color: black;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  margin-bottom: 50px;
} 